	<parameter name="FECHA_INI" isForPrompting="false" class="java.util.Date"/>
	<parameter name="FECHA_FIN" isForPrompting="false" class="java.util.Date"/>
	<parameter name="YEAR" isForPrompting="true" class="java.lang.String"/>
	<parameter name="MES" isForPrompting="true" class="java.lang.String"/>
	<parameter name="EMPRESA_ID" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT c.id,S.EJERCICIO as YEAR,S.MES,'ENERO' AS MES_NOMBRE,C.TIPO,C.SUB_TIPO,C.DE_RESULTADO,C.DESCRIPCION,C.CLAVE
,CASE WHEN C.TIPO='ACTIVO' THEN '1' ELSE '2'  END AS GRUPO,(S.SALDO_FINAL) AS SALDO
FROM cuenta_contable C
JOIN saldo_por_cuenta_contable S ON(S.CUENTA_ID=C.ID)
WHERE S.EJERCICIO=$P{YEAR} AND S.mes=$P{MES} AND S.EMPRESA_ID=$P{EMPRESA_ID} AND C.TIPO<>'ORDEN' AND C.PADRE_ID IS NULL AND C.de_resultado IS FALSE  AND C.CLAVE <>'109-0000' 
	UNION
SELECT c.id,S.EJERCICIO as YEAR,S.MES,'ENERO' AS MES_NOMBRE,C.TIPO,C.SUB_TIPO,C.DE_RESULTADO,'PAGOS ANTICIPADOS' AS DESCRIPCION,C.CLAVE
,CASE WHEN C.TIPO='ACTIVO' THEN '1' ELSE '2'  END AS GRUPO,SUM(S.SALDO_FINAL) AS SALDO
FROM cuenta_contable C
JOIN saldo_por_cuenta_contable S ON(S.CUENTA_ID=C.ID)
WHERE S.EJERCICIO=$P{YEAR} AND S.mes=$P{MES} AND S.EMPRESA_ID=$P{EMPRESA_ID} AND C.TIPO<>'ORDEN'  AND C.CLAVE ='109-0000' 
	UNION
SELECT C.id,YEAR(P.FECHA) AS YEAR,MONTH(P.FECHA) AS MES,'ENERO' AS MES_NOMBRE,C.TIPO,C.SUB_TIPO,C.de_resultado,'RESULTADO DEL EJERCICIO' AS DESCRIPCION,C.CLAVE
,(CASE WHEN C.TIPO='ACTIVO' THEN '1' ELSE '2'  END) AS GRUPO ,IFNULL(SUM(D.DEBE-D.HABER),0) AS SALDO
FROM poliza_det d JOIN poliza P ON(P.id=D.poliza_id) JOIN cuenta_contable C ON(C.ID=D.cuenta_id) 
where   YEAR(P.FECHA)=$P{YEAR} AND P.EMPRESA_ID=$P{EMPRESA_ID} AND MONTH(P.FECHA)<=$P{MES} AND P.SUB_TIPO NOT LIKE '%CIERRE%ANUAL%'
 AND C.TIPO<>'ORDEN' 
 AND C.de_resultado is true]]></queryString>


	<parameter name="NomReporte" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["COMPLEMENTO DE POLIZA DE METODO DE PAGO"]]></defaultValueExpression>
	</parameter>
	<parameter name="POLIZA_ID" isForPrompting="true" class="java.lang.Long"/>
	<parameter name="EMPRESA" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["BERSAN,  S.A. DE C.V."]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT 'TRANSFERENCIA' as fpago,t.id,e.id as eId,e.nombre as empresa,p.id as poliza_id,p.tipo,p.sub_tipo
,p.folio,p.fecha
,t.cuenta_origen,t.fecha,t.beneficiario,t.rfc,t.monto as monto_total,t.moneda,t.tipo_de_cambio
,(SELECT b.nombre_corto from banco_sat b where b.id=t.banco_origen_nacional_id) as banco_ori_nal
,t.banco_origen_extranjero,t.cuenta_destino
,(SELECT b.nombre_corto from banco_sat b where b.id=t.banco_destino_nacional_id) as banco_dest_nal
,t.banco_destino_extranjero
FROM transaccion_transferencia t
join poliza_det d on(d.id=t.poliza_det_id)
join poliza p on(p.id=d.poliza_id)
join empresa e on(e.id=p.empresa_id)
where p.id=$P{POLIZA_ID}
group by t.cuenta_origen,t.fecha,t.beneficiario,t.rfc,t.monto,t.moneda,t.tipo_de_cambio
union
SELECT 'CHEQUE' as fpago,t.id,e.id as eId,e.nombre as empresa,p.id as poliza_id,p.tipo,p.sub_tipo
,p.folio,p.fecha
,t.cuenta_origen,t.fecha,t.beneficiario,t.rfc,t.monto as monto_total,t.moneda,t.tipo_de_cambio
,(SELECT b.nombre_corto from banco_sat b where b.id=t.banco_emisor_nacional_id) as banco_ori_nal
,t.banco_emisor_extranjero
,'' as cuenta_destino
,'' as banco_dest_nal
,'' as banco_destino_extranjero
FROM poliza_cheque t
join poliza_det d on(d.id=t.poliza_det_id)
join poliza p on(p.id=d.poliza_id)
join empresa e on(e.id=p.empresa_id)
where p.id=$P{POLIZA_ID}
group by t.cuenta_origen,t.fecha,t.beneficiario,t.rfc,t.monto,t.moneda,t.tipo_de_cambio]]></queryString>


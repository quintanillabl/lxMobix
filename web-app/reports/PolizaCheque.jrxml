	<queryString><![CDATA[SELECT ch.ID,M.FECHA,UPPER(m.a_favor) AS AFAVOR,-m.importe as IMPORTE,CH.FOLIO,c.nombre as BANCO,R.COMENTARIO
FROM cheque ch 
join movimiento_de_cuenta m on(m.id=ch.egreso_id) 
join requisicion r on(r.id=m.requisicion_id) 
join proveedor p on(p.id=r.proveedor_id)
join cuenta_bancaria c on(c.id=m.cuenta_id)
where ch.id=$P{ID}]]></queryString>


	<queryString><![CDATA[SELECT t.id,e.id as eId,e.nombre as empresa,p.id as poliza_id,p.tipo,p.sub_tipo
,p.folio,p.fecha,t.uuidcfdi,t.rfc,t.monto_total,t.moneda,t.tip_camb 
FROM transaccion_compra_nal t
join poliza_det d on(d.id=t.poliza_det_id)
join poliza p on(p.id=d.poliza_id)
join empresa e on(e.id=p.empresa_id)
where p.id=$P{POLIZA_ID}  
group by p.folio,p.id,t.uuidcfdi,t.rfc,t.monto_total,t.moneda,t.tip_camb]]></queryString>


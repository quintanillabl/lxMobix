JOIN cuenta_contable C ON(C.ID=S.CUENTA_ID)  
JOIN cuenta_contable X ON(X.ID=C.PADRE_ID) 
JOIN CUENTA_SAT Y ON(Y.ID=C.cuenta_sat_id)
JOIN CUENTA_SAT Z ON(Z.ID=X.cuenta_sat_id)
WHERE S.EJERCICIO=$P{YEAR} AND S.MES=$P{MES}
ORDER BY X.CLAVE,C.CLAVE]]></queryString>

